ment;\r\n        },\r\n        cancelDeleteDepartment: function(){ \r\n           this.stateDeleteDepartament = false;\r\n           this.objectDepartament = undefined;       \r\n        }, \r\n        deleteDepartment: function(){          \r\n            this.$store\r\n            .dispatch(\"deleteDepartment\", this.objectDepartament.id)\r\n            .then(() => this.cancelDeleteDepartment())\r\n            .catch(e => {\r\n                alert(\"Error al actualizar el departamento\");\r\n            console.log(e);\r\n            });\r\n\r\n        }\r\n    },\r\n\tcomputed: {\r\n\t\tdepartments() {\r\n\t\t\treturn Object.values(this.$store.getters[\"departments\"]).map(dep => {\r\n        dep.nestado = dep.estado?\"Activo\":\"Inactivo\";\r\n        return dep;\r\n      });\r\n\t\t}\r\n\t}\r\n};\r\n</script>\r\n\r\n<style>\r\n.main{\r\n    margin: 3em;\r\n}\r\n</style>\r\n"],"sourceRoot":"src/components/departments"}]}